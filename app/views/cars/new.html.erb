<%= form_for @car do |f| %>
  <p>
    <%= f.label :model %><br />
    <%= f.text_field :model %>
  </p>
  <p>
    <%= f.label :year %><br />
    <%= f.number_field :year %>
  </p>
  <p>
    <%= f.label :color %><br />
    <%= f.text_field :color %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
